# Auto-created environment variables for local Airflow compose
# Admin UI user created by the official Airflow image entrypoint
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin

# Airflow runtime user (keeps files owned by your host user)
AIRFLOW_UID=501
AIRFLOW_GID=0

# Postgres credentials used by the Postgres service and by Airflow
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow
POSTGRES_DB=warehouse

# Optional: additional requirements to install at build time (kept empty here)
# _PIP_ADDITIONAL_REQUIREMENTS=

# Neo4j credentials (used by neo4j service and Airflow DAGs)
NEO4J_AUTH=neo4j/woz12345
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=woz12345

# Redis defaults
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
